<configuration>

    <appender name="STDOUT" class="ch.qos.logback.core.ConsoleAppender">
        <encoder>
            <pattern>%date %-5level [%thread] - [%logger]- %msg%n</pattern>
        </encoder>
    </appender>
    
    
    <logger name="org.springframework">
		<level value="INFO" />
	</logger>
	<logger name="org.hibernate">
		<level value="INFO" />
	</logger>

    <root level="INFO">
        <appender-ref ref="STDOUT" />
    </root>
</configuration>